{
    "_form": {
        "ui": [
            [
                {
                    "field": "name",
                    "optionItem": {
                        "label": "名称",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 9
                        },
                        "wrapperCol": {
                            "span": 15
                        }
                    },
                    "span": 8,
                    "optionJsx": {
                        "placeholder": "请输入名称"
                    },
                    "optionConfig": {
                        "normalize": "text,40",
                        "rules": [
                            {
                                "required": true,
                                "message": "请输入名称，名称不可为空！"
                            }
                        ]
                    },
                    "render": "aiInput"
                },
                {
                    "field": "password",
                    "optionItem": {
                        "label": "密码",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 8
                        },
                        "wrapperCol": {
                            "span": 16
                        }
                    },
                    "render": "aiInput",
                    "span": 8
                }
            ],
            [
                {
                    "field": "username1",
                    "optionItem": {
                        "label": "用户名A",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 9
                        },
                        "wrapperCol": {
                            "span": 15
                        }
                    },
                    "render": "aiInput",
                    "span": 8
                },
                {
                    "span": 16,
                    "field": "username2",
                    "optionItem": {
                        "label": "用户名B",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 4
                        },
                        "wrapperCol": {
                            "span": 20
                        }
                    },
                    "render": "aiInput"
                }
            ],
            [
                {
                    "optionJsx": {
                        "config": {
                            "items": [
                                {
                                    "key": "fixedvalue1",
                                    "value": "fixedvalue1",
                                    "label": "display1"
                                },
                                {
                                    "key": "fixedvalue2",
                                    "value": "fixedvalue2",
                                    "label": "display2"
                                }
                            ]
                        }
                    },
                    "optionItem": {
                        "label": "下拉",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 9
                        },
                        "wrapperCol": {
                            "span": 15
                        }
                    },
                    "optionConfig": {},
                    "field": "status",
                    "span": 8,
                    "render": "aiSelect"
                },
                {
                    "optionJsx": {
                        "config": {
                            "expr": "expr",
                            "datum": {
                                "source": "ci.environment",
                                "value": "value",
                                "label": "display"
                            },
                            "cascade": {
                                "source": "filter",
                                "target": "BOOL:true"
                            }
                        }
                    },
                    "optionItem": {
                        "label": "下拉2",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 8
                        },
                        "wrapperCol": {
                            "span": 16
                        }
                    },
                    "optionConfig": {},
                    "field": "environment",
                    "span": 8,
                    "render": "aiSelect"
                }
            ],
            [
                {
                    "optionJsx": {
                        "config": {
                            "linker": {
                                "name1": "username1",
                                "name2": "username2"
                            }
                        }
                    },
                    "optionItem": {
                        "label": "标签信息",
                        "style": {
                            "width": "100%",
                            "marginLeft": 0,
                            "paddingLeft": 0
                        },
                        "labelCol": {
                            "span": 9
                        },
                        "wrapperCol": {
                            "span": 15
                        }
                    },
                    "optionConfig": {},
                    "field": "environment",
                    "render": "aiListSelector",
                    "span": 8
                }
            ]
        ],
        "hidden": [
            "username1"
        ],
        "op": {
            "$opAdd": "act.category.add"
        },
        "initial": {
            "identifier": "PROP:$identifier",
            "active": "BOOL:true"
        },
        "assist": {
            "resource.departments": {
                "uri": "/api/dept/by/sigma"
            },
            "data.category": {
                "uri": "/api/type/categories/:type",
                "magic": {
                    "type": "FIX:ci.type"
                }
            },
            "model.identifiers": {
                "uri": "/api/model"
            },
            "resource.employees": {
                "uri": "/api/employee/by/sigma"
            },
            "resource.customers": {
                "uri": "/api/customer/by/sigma"
            },
            "resource.teams": {
                "uri": "/api/team/by/sigma"
            },
            "security.users": {
                "uri": "/api/user/by/sigma"
            },
            "tabular": {
                "uri": "/api/types/tabulars",
                "method": "POST",
                "magic": {
                    "$body": [
                        "ci.environment",
                        "ci.status"
                    ]
                },
                "group": "type"
            },
            "category": {
                "uri": "/api/types/categories",
                "method": "POST",
                "magic": {
                    "$body": []
                },
                "group": "type"
            }
        },
        "columns": 3,
        "window": 1,
        "options": {
            "window": 1
        }
    },
    "modal": {
        "success": {
            "added": "您好，您的信息添加成功！"
        }
    }
}