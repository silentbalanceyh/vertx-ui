{
    "_form": {
        "window": 1,
        "columns": 4,
        "ui": [
            [
                "title=添加参数详细信息"
            ],
            [
                {
                    "metadata": "name,参数名称,12,,,placeholder=（输入您要设置的参数名）",
                    "optionConfig.rules": [
                        "required,请填写参数名称，参数名称不可为空"
                    ]
                }
            ],
            [
                {
                    "metadata": "type,参数类型,24,,aiRadio",
                    "optionItem.className": "web-radio-two",
                    "optionJsx.config.items": [
                        "BOOL,逻辑值",
                        "FIX,固定值",
                        "ENUM,枚举值",
                        "UNIQUE,字典值",
                        "USER,当前登录用户",
                        "FORM,表单值",
                        "ROUTE,路由值",
                        "PROP,属性值（React组件）",
                        "STATE,状态值（React组件）"
                    ]
                }
            ],
            [
                {
                    "metadata": "value,参数值,24",
                    "optionJsx.config": {
                        "empty": "（请先选择参数类型，再设置参数值！）"
                    }
                }
            ],
            [
                {
                    "metadata": "$button",
                    "optionJsx.extension": [
                        "$opSaveParam,追加参数,SUBMIT,primary"
                    ],
                    "span": 24
                }
            ]
        ]
    }
}