                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleMaxMessage,提示信息,24,,,,inscribe=最大值验证失败时的提示信息",
                                            "optionJsx.depend.enabled": {
                                                "ruleMax": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleMin,最小值规则,8,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "启用",
                                                "unCheckedChildren": "禁用"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        },
                                        {
                                            "metadata": "ruleMinValid,允许最小值,16,100%,aiInputNumber",
                                            "optionJsx.depend.enabled": {
                                                "ruleMin": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleMinMessage,提示信息,24,,,,inscribe=最小值验证失败时的提示信息",
                                            "optionJsx.depend.enabled": {
                                                "ruleMin": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleLen,定长规则,8,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "启用",
                                                "unCheckedChildren": "禁用"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        },
                                        {
                                            "metadata": "ruleLenValid,固定长度,16,100%,aiInputNumber",
                                            "optionJsx.depend.enabled": {
                                                "ruleLen": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleLenMessage,提示信息,24,,,,inscribe=定长规则验证失败时的提示信息",
                                            "optionJsx.depend.enabled": {
                                                "ruleLen": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleReg,正则规则,8,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "启用",
                                                "unCheckedChildren": "禁用"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        },
                                        {
                                            "metadata": "ruleRegValid,正则表达式,16,100%",
                                            "optionJsx.depend.enabled": {
                                                "ruleReg": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleRegMessage,提示信息,24,,,,inscribe=正则规则验证失败时的提示信息",
                                            "optionJsx.depend.enabled": {
                                                "ruleReg": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleEqual,等于规则,8,,aiCheckbox,valuePropName=checked",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "启用",
                                                "unCheckedChildren": "禁用"
                                            }
                                        },
                                        {
                                            "metadata": "ruleEqualTo,目标字段,16,,aiSelect",
                                            "optionJsx.config.datum": "source=model.fields,key=key,label=display",
                                            "optionJsx.depend.enabled": {
                                                "ruleEqual": true
                                            },
                                            "optionConfig.rules": [
                                                {
                                                    "validator": "diff",
                                                    "message": "对不起，比对字段不可和原字段相等！",
                                                    "config": {
                                                        "to": "field"
                                                    }
                                                }
                                            ]
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleEqualMessage,提示信息,24,,,,inscribe=等于规则验证失败时的显示信息",
                                            "optionJsx.depend.enabled": {
                                                "ruleEnum": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleEnum,枚举规则,,,aiCheckbox,readOnly=true",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "启用",
                                                "unCheckedChildren": "禁用"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleEnumValid,合法枚举,24,100%,aiInputMulti",
                                            "optionJsx.styleInput": {
                                                "width": "30%"
                                            },
                                            "optionJsx.depend.enabled": {
                                                "ruleEnum": true
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "ruleEnumMessage,提示信息,24,,,,inscribe=枚举规则验证失败时的提示信息",
                                            "optionJsx.depend.enabled": {
                                                "ruleEnum": true
                                            }
                                        }
                                    ]
                                ]
                            }
                        }
                    }
                }
            ]
        ]
    }
}