{
    "_grid": {
        "module": {
            "NAME": "公告",
            "MODULE": "x-notice",
            "IDENTIFIER": "x.notice"
        },
        "options": {
            "search.cond": [
                "subject,c"
            ],
            "search.placeholder": "公告标题",
            "search.advanced": false,
            "op.extension.generate": {
                "text": "预览",
                "icon": "info",
                "region": "op.row",
                "config": {
                    "executor": "rxPreview"
                }
            }
        },
        "table": {
            "columns": [
                {
                    "title": "操作",
                    "dataIndex": "key",
                    "fixed": "left",
                    "width": 140,
                    "$render": "EXECUTOR",
                    "$option": [
                        {
                            "key": "$opSave",
                            "text": "编辑",
                            "executor": "fnEdit"
                        },
                        {
                            "key": "$opPreview",
                            "text": "预览",
                            "executor": "fnPreview"
                        }
                    ]
                }
            ]
        }
    }
}