{
    "_page": {
        "title": "文件申请/审批",
        "back": {
            "state": {
                "status.submitting": null
            }
        }
    },
    "_tabs": {
        "className": "ex-tabs-tab-block",
        "tabPosition": "left",
        "defaultActiveKey": "tabBefore",
        "items": "申请队列,tabBefore,audit;已完成,tabApproved,history"
    },
    "_workflow": {
        "parameter": {
            "definitionKey": "process.file.management"
        },
        "record": {
            "initial": "rxAttachment"
        },
        "canvas": {
            "height": 240
        },
        "decision": {
            "$opDraft": {
                "eStart": {
                    "draft": true,
                    "status": "DRAFT"
                }
            },
            "$opMove": {
                "eStart": {
                    "draft": false,
                    "status": "PENDING"
                }
            },
            "$opApprove": {
                "eFileApprove": {
                    "confirmed": true
                }
            },
            "$opReject": {
                "eFileApprove": {
                    "confirmed": false
                }
            }
        },
        "options": {
            "tabs.list": "文件申请列表",
            "op.open.add": "申请"
        }
    }
}