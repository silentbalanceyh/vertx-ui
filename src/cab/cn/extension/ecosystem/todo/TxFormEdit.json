{
    "_form": {
        "modal": {
            "success": {
                "saving": "您好，您已成功保存草稿！申请单号 :serial。",
                "start": "您好，您的流程申请已成功提交，申请单号 :serial，请等待审批。",
                "approve": "您好，该申请已成功审批通过！！",
                "reject": "您好，您已拒绝该申请！",
                "cancel": "您好，您已拒成功取消了该申请！"
            }
        },
        "ui": [
            [
                {
                    "metadata": "$button",
                    "hidden": true,
                    "optionJsx.extension": [
                        "$opSaving,存草稿,SUBMIT,primary,,,closable=true,callback=saving",
                        "$opStart,提交申请,SUBMIT,primary,,,closable=true,callback=start",
                        "$opApprove,审批,SUBMIT,primary,,,closable=true,callback=approve",
                        "$opReject,拒绝,SUBMIT,primary,,,closable=true,callback=reject",
                        "$opCancel,取消,SUBMIT,primary,,,closable=true,callback=cancel",
                        "$opReset,重置,RESET"
                    ],
                    "span": 24
                }
            ],
            [
                "title=业务信息"
            ],
            [
                "serial,待办编号,8,,,readOnly=true,inscribe=系统自动生成",
                {
                    "metadata": "typeName,服务目录,8,,aiTreeSelector,placeholder=（选填，统计用）",
                    "optionJsx.config": {
                        "ajax": {
                            "uri": "/api/type/categories/:type",
                            "magic": {
                                "type": "FIX:zero.service.catalog"
                            }
                        },
                        "linker": {
                            "name": "typeName",
                            "code": "type"
                        },
                        "selection": {
                            "multiple": false,
                            "checkStrictly": true,
                            "limitOp": "EQ",
                            "limitConfig": {
                                "leaf": true
                            }
                        },
                        "tree": {
                            "title": "name"
                        },
                        "validation": "请选择服务目录！",
                        "window": "选择服务目录,选择,关闭,false,400,false"
                    },
                    "optionJsx.allowClear": true
                },
                {
                    "metadata": "status,状态,8,,aiMagic",
                    "optionJsx.config.items": [
                        "DRAFT,草稿:form,16:#EE3B3B",
                        "PENDING,等待处理:undo,16:#EE3B3B",
                        "FINISHED,已完成:check-circle,16:#58c623",
                        "ACCEPTED,已接受:info-circle,16:#58c623",
                        "REJECTED,已拒绝:redo,16:#CD1076",
                        "CANCELED,已撤销:stop,16:#CD1076",
                        "EXPIRED,已超时:clock-circle,16:#CD1076"
                    ]
                }
            ],
            [
                {
                    "metadata": "name,申请标题,16,99%,,placeholder=请填写您的申请标题",
                    "optionConfig.rules": [
                        "required,请填写申请标题，标题不可为空！"
                    ]
                },
                {
                    "metadata": "userName,指定用户,8,,aiListSelector,placeholder=（选择用户）",
                    "optionJsx.config": {
                        "ajax": {
                            "metadata": "POST,/api/user/search,1,10,sorter=createdAt`ASC",
                            "params.criteria": {
                                "sigma": "PROP:app.sigma",
                                "key,<>": "USER:key",
                                "": true
                            }
                        },
                        "linker": {
                            "realname": "userName",
                            "key": "toUser"
                        },
                        "table": {
                            "columns": [
                                {
                                    "title": "部门",
                                    "dataIndex": "deptId",
                                    "$render": "USER",
                                    "$config": {
                                        "uri": "/api/dept/:key",
                                        "field": "name",
                                        "icon": false
                                    },
                                    "$empty": "（无）"
                                },
                                "workNumber,员工号",
                                "username,登录账号",
                                "realname,姓名",
                                "workPhone,工作电话"
                            ]
                        },
                        "validation": "请选择用户！",
                        "window": "选择用户,选择,关闭,false,800,false",
                        "search": {
                            "username,c": "账号",
                            "realname,c": "姓名"
                        }
                    },
                    "optionJsx.allowClear": true
                }
            ],
            [
                "comment,描述信息,24,,aiTextArea,rows=3"
            ]
        ],
        "hidden": [
            "type",
            "deptId",
            "teamId",
            "toGroup",
            "toRole",
            "toUser",
            "key",
            "traceId",
            "traceTaskId"
        ]
    },
    "_formDown": {
        "ui": [
            [
                "title=分派信息"
            ],
            [
                {
                    "metadata": "toGroupMode,派工对象,16,,aiRadio",
                    "optionJsx.config.items": [
                        "NONE,（无特定）",
                        "ROLE,角色",
                        "DEPT,部门",
                        "TEAM,业务组",
                        "GROUP,用户组"
                    ],
                    "optionJsx.depend.impact": {
                        "reset": [
                            "groupName",
                            "toGroup",
                            "roleName",
                            "toRole",
                            "deptId",
                            "deptName",
                            "teamId",
                            "teamName"
                        ]
                    }
                }
            ],
            [
                {
                    "metadata": "deptName,部门,8,,aiTreeSelector,placeholder=（选部门）",
                    "optionJsx.config": {
                        "ajax": {
                            "uri": "/api/dept/search",
                            "method": "POST",
                            "params.criteria": {
                                "companyId": "USER:companyId"
                            }
                        },
                        "linker": {
                            "name": "deptName",
                            "key": "deptId"
                        },
                        "selection": {
                            "multiple": false,
                            "checkStrictly": true
                        },
                        "tree": {
                            "title": "name",
                            "parent": "deptId",
                            "pinyin": true
                        },
                        "validation": "请选择部门！",
                        "window": "选择部门,选择,关闭,false,400,false"
                    },
                    "optionJsx.allowClear": true,
                    "optionJsx.depend.enabled": {
                        "toGroupMode": "DEPT"
                    }
                },
                {
                    "metadata": "teamName,业务组,8,,aiTreeSelector,placeholder=（选业务组）",
                    "optionJsx.config": {
                        "ajax": {
                            "uri": "/api/team/search",
                            "method": "POST",
                            "params.criteria": {
                                "sigma": "USER:sigma"
                            }
                        },
                        "linker": {
                            "name": "teamName",
                            "key": "teamId"
                        },
                        "selection": {
                            "multiple": false,
                            "checkStrictly": true
                        },
                        "tree": {
                            "title": "name",
                            "parent": "teamId",
                            "pinyin": true
                        },
                        "validation": "请选择业务组！",
                        "window": "选择业务组,选择,关闭,false,400,false"
                    },
                    "optionJsx.allowClear": true,
                    "optionJsx.depend.enabled": {
                        "toGroupMode": "TEAM"
                    }
                },
                {
                    "metadata": "groupName,用户组,8,,aiTreeSelector,placeholder=（选用户组）",
                    "optionJsx.config": {
                        "ajax": {
                            "uri": "/api/group/search",
                            "method": "POST",
                            "params.criteria": {
                                "sigma": "USER:sigma"
                            }
                        },
                        "linker": {
                            "name": "groupName",
                            "key": "toGroup"
                        },
                        "selection": {
                            "multiple": false,
                            "checkStrictly": true
                        },
                        "tree": {
                            "title": "name",
                            "parent": "parentId",
                            "pinyin": true
                        },
                        "validation": "请选择用户组！",
                        "window": "选择用户组,选择,关闭,false,400,false"
                    },
                    "optionJsx.allowClear": true,
                    "optionJsx.depend.enabled": {
                        "toGroupMode": "GROUP"
                    }
                }
            ],
            [
                {
                    "metadata": "roleName,角色,8,,aiListSelector,placeholder=（选角色）",
                    "optionJsx.allowClear": true,
                    "optionJsx.config": {
                        "ajax": {
                            "metadata": "POST,/api/role/search,1,10,sorter=name`ASC",
                            "params.criteria": {
                                "sigma": "PROP:app.sigma"
                            }
                        },
                        "linker": {
                            "name": "roleName",
                            "key": "toRole"
                        },
                        "table": {
                            "columns": [
                                "code,角色编码",
                                "name,角色名称",
                                {
                                    "metadata": "super,超级管理员,LOGICAL",
                                    "$mapping": {
                                        "true": "是",
                                        "false": "否"
                                    }
                                },
                                {
                                    "metadata": "updatedAt,更新时间,DATE",
                                    "$format": "YYYY-MM-DD"
                                }
                            ]
                        },
                        "validation": "请选择角色！",
                        "window": "选择角色,选择,关闭,false,900,false",
                        "search": {
                            "name,c": "名称",
                            "code,c": "编码"
                        }
                    },
                    "optionJsx.depend.enabled": {
                        "toGroupMode": "ROLE"
                    }
                }
            ]
        ]
    },
    "_title": {
        "workflow": "流程详情"
    }
}