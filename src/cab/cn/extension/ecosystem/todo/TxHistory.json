                    "REQUESTED": "申请,edit,,#8FBC8F",
                    "FINISHED": "已完成,check-circle,,#698B69",
                    "REJECTED": "被拒绝,redo,,#CD1076",
                    "CANCELED": "已撤销,stop,,#CD1076"
                },
                "width": 100
            },
            {
                "metadata": "opBy,操作人,USER",
                "$config": {
                    "uri": "/api/user/:key",
                    "field": "realname",
