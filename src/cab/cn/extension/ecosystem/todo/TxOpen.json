{
    "_form": {
        "columns": 3,
        "assist": {
            "resource.departments": {
                "uri": "/api/dept/by/sigma",
                "inherit": "resource.departments"
            },
            "resource.teams": {
                "uri": "/api/team/by/sigma",
                "inherit": "resource.teams"
            },
            "ajax.groups": {
                "uri": "/api/group/by/sigma",
                "inherit": true
            }
        },
        "modal": {
            "success": {
                "open": "您好，您的流程申请已成功提交，申请单号 :serial，请等待审批。",
                "draft": "您好，您的流程申请已成功保存为草稿！单号为 :serial。"
            }
        },
        "segment": {
            "TICKET_OPEN": "OPEN_USER"
        },
        "ui": [
            [
                {
                    "metadata": "$button",
                    "optionJsx.grouped": true,
                    "optionItem.label": "流程操作",
                    "optionJsx.extension": [
                        "$opStart,提交,SUBMIT,primary,,upload,closable=true,callback=open",
                        "$opDraft,暂存,SUBMIT,,uc_pink,,closable=true,callback=draft",
                        "$opReset,重置,RESET",
                        "$opBack,返回,BACK"
                    ],
                    "span": 24
                }
            ],
            [
                {
                    "complex": true,
                    "name": "FieldContainer",
                    "key": "monitorContainer",
                    "config": {
                        "activeKey": "ticket",
                        "size": "small",
                        "items": "主单信息,ticket;流程信息,bpmn;关联资产,linkageAsset;关联工单,linkageTicket;关联员工,linkageEmployee;关联合同,linkageContract;关联项目,linkageProject;关联客户,linkageCustomer",
                        "pages": {
                            "ticket": {
                                "ui": [
                                    [
                                        "title=基本信息"
                                    ],
                                    [
                                        "serial,单号,8,,,readOnly=true,inscribe=系统自动生成",
                                        "phase,工单状态,8",
                                        {
                                            "metadata": "openAt,制单时间,8,,aiMagic",
                                            "optionJsx.config.format": "YYYY年MM月DD日 HH:mm",
                                            "moment": true
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "title,工单标题,24,,,placeholder=请填写您的工单标题",
                                            "optionConfig.rules": [
                                                "required,请填写工单标题，标题不可为空！"
                                            ]
                                        }
                                    ],
                                    [
                                        "description,详细描述,24,,aiBraftEditor"
                                    ],
                                    "TICKET",
                                    "TICKET_OPEN",
                                    [
                                        "title=下一处理人"
                                    ],
                                    "ASSIGNMENT",
                                    "ASSIGNMENT_MORE",
                                    [
                                        "title=附件信息"
                                    ],
                                    [
                                        {
                                            "metadata": "linkageAttachment,文件上传,18,,aiFileBatch,text=上传",
                                            "optionJsx.accept": "*/*",
                                            "optionJsx.config.filekey": "key",
                                            "optionJsx.config.limit": 10240,
                                            "optionJsx.config.reduce": true,
                                            "optionJsx.ajax.uri": "/api/file/upload/:identifier?category=:category&directory=:directory",
                                            "optionJsx.ajax.download": "/api/file/download/:key",
                                            "optionJsx.ajax.params": {
                                                "identifier": "FIX:w.ticket",
                                                "category": "PROP:workflow.code",
                                                "directory": "/流程文档",
                                                "formula": "/${catalogPath}/${serial}"
                                            }
                                        }
                                    ]
                                ]
                            },
                            "bpmn": {
                                "ui": [
                                    [
                                        "title=流程详情"
                                    ],
                                    [
                                        "monitorBpmn,,24"
                                    ],
                                    [
                                        "title=操作历史"
                                    ],
                                    [
                                        "monitorHistory,,24"
                                    ]
                                ]
                            },
                            "linkageTicket": {
                                "ui": [
                                    [
                                        "title=设置关联工单"
                                    ],
                                    [
                                        "linkageTicket,,24"
                                    ]
                                ]
                            },
                            "linkageAsset": {
                                "ui": [
                                    [
                                        "title=设置关联资产"
                                    ],
                                    [
                                        "linkageAsset,,24"
                                    ]
                                ]
                            },
                            "linkageEmployee": {
                                "ui": [
                                    [
                                        "title=设置关联员工"
                                    ],
                                    [
                                        "linkageEmployee,,24"
                                    ]
                                ]
                            },
                            "linkageContract": {
                                "ui": [
                                    [
                                        "title=设置关联合同"
                                    ],
                                    [
                                        "linkageContract,,24"
                                    ]
                                ]
                            },
                            "linkageProject": {
                                "ui": [
                                    [
                                        "title=设置关联项目"
                                    ],
                                    [
                                        "linkageProject,,24"
                                    ]
                                ]
                            },
                            "linkageCustomer": {
                                "ui": [
                                    [
                                        "title=设置关联客户"
                                    ],
                                    [
                                        "linkageCustomer,,24"
                                    ]
                                ]
                            }
                        }
                    }
                }
            ]
        ],
        "hidden": [
            "type",
            "catalog",
            "owner",
            "supervise",
            "openBy",
            "groupOpen",
            "groupNext",
            "toUser",
            "toDept",
            "toTeam",
            "toGroup",
            "toRole"
        ],
        "initial": {
            "phase": "INIT",
            "openBy": "USER:key",
            "owner": "USER:key"
        }
    }
}