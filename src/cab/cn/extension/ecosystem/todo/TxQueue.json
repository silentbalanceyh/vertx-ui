            "columns": 3,
            "ui": [
                [
                    "title=流程基本信息"
                ],
                [
                    "name,流程名,8,,aiMagic,readOnly=true",
                    "definitionKey,流程代码,8,,aiMagic,readOnly=true",
                    "definitionId,实例ID,8,,aiMagic,readOnly=true"
                ]
            ]
        }
    },
