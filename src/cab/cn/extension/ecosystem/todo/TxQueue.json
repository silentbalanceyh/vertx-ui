                "fieldColumn": {
                    "title": "选择属性",
                    "items": [
                        "active"
                    ]
                },
                "valueColumn": {
                    "title": "设置值",
                    "render": {
                        "active": "RADIO"
                    }
                }
            }
        },
        "table": {
            "total": {
                "report": "（总共:page页，:total条数据）",
                "selected": "勾选了:count条"
            },
            "row": {
                "onDoubleClick": "fnEdit"
            },
            "columns": [
                {
                    "title": "操作",
                    "dataIndex": "key",
                    "fixed": "left",
                    "$render": "EXECUTOR",
                    "$option": [
                        {
                            "text": "编辑",
                            "executor": "fnEdit"
                        },
                        "divider",
                        {
                            "text": "删除",
                            "executor": "fnDelete",
                            "confirm": "确认删除选择的申请记录记录？"
                        }
                    ]
                }
            ]
        }
    }
}