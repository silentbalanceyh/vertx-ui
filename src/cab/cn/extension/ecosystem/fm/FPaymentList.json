{
    "_assist": {
        "tabular": {
            "uri": "/api/types/tabulars",
            "method": "POST",
            "magic": {
                "$body": [
                    "pay.type"
                ]
            },
            "group": "type"
        }
    },
    "_table": {
        "className": "ux_table",
        "pagination": false,
        "columns": [
            {
                "title": "操作",
                "dataIndex": "key",
                "fixed": "left",
                "width": 100,
                "config": {
                    "confirm": "该操作将删除本次付款，确认？",
                    "text": "删除"
                }
            },
            {
                "title": "编号",
                "dataIndex": "serial",
                "sorter": true
            },
            "amount,金额,CURRENCY",
            {
                "metadata": "payMethod,支付方式,DATUM",
                "$datum": "source=pay.type,value=code,display=name"
            },
            {
                "title": "处理时间",
                "dataIndex": "updatedAt",
                "sorter": true,
                "$render": "DATE",
                "$format": "YYYY-MM-DD HH:mm:ss"
            }
        ]
    }
}