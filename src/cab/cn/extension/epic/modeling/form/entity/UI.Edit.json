{
    "_form": {
        "window": 1,
        "ui": [
            [
                "title=实体信息"
            ],
            [
                {
                    "metadata": "appName,名空间,12,,,readOnly=true",
                    "optionJsx.addonBefore": "cn.originx."
                },
                {
                    "metadata": "identifier,全局ID,12,,,placeholder=（使用形如\"xx.yy\"的格式，如ci.device）,readOnly=true",
                    "optionConfig.rules": [
                        "required,请输入全局ID，不可为空！",
                        {
                            "validator": "existing",
                            "message": "对不起，全局ID重复！",
                            "config": {
                                "uri": "/api/entity/existing",
                                "method": "POST",
                                "params": {
                                    "namespace": "FORM:namespace"
                                }
                            }
                        }
                    ]
                }
            ],
            [
                {
                    "metadata": "name,系统名称,12",
                    "optionConfig.rules": [
                        "required,请输入系统名称，该名称不可为空！"
                    ]
                },
                {
                    "metadata": "tableName,数据库表名,12,,,placeholder=（根据规范表名全大写，如RES_COMPANY）,readOnly=true",
                    "optionConfig.rules": [
                        "required,请输入表名，表名不可为空！",
                        {
                            "validator": "existing",
                            "message": "对不起，数据库表名重复！",
                            "config": {
                                "uri": "/api/entity/existing",
                                "method": "POST",
                                "params": {
                                    "namespace": "FORM:namespace"
                                }
                            }
                        }
                    ]
                }
            ],
            [
                {
                    "metadata": "type,模型类型,12,,aiRadio",
                    "optionJsx.config.items": [
                        "ENTITY,实体模型",
                        "RELATION,关系模型"
                    ]
                },
                {
                    "metadata": "active,启用,12,,aiCheckbox",
                    "optionConfig": {
                        "valuePropName": "checked"
                    }
                }
            ],
            [
                "comments,备注,24,,aiTextArea,rows=3"
            ],
            [
                "title=详细信息"
            ],
            [
                {
                    "complex": true,
                    "name": "FieldContainer",
                    "config": {
                        "activeKey": "tabField",
                        "items": "字段设置,tabField;键设置,tabKey",
                        "tabBarExtraContent": {
                            "tabField": [
                                "$opAddField,添加字段,$opShowField,primary,plus"
                            ],
                            "tabKey": [
                                "$opAddKey,添加键,$opShowKey,primary,plus"
                            ]
                        },
                        "pages": {
                            "tabField": {
                                "ui": [
                                    [
                                        {
                                            "metadata": "fields,,24,,aiDialogEditor",
                                            "optionJsx.config": {
                                                "form": "FormField",
                                                "dialog": "字段设置,保存,关闭,false,900,true,$opSaveField",
                                                "op": {
                                                    "add": "$opShowField"
                                                },
                                                "table": {
                                                    "columns": [
                                                        {
                                                            "title": "操作",
                                                            "dataIndex": "key",
                                                            "fixed": "left",
                                                            "$render": "EXECUTOR",
                                                            "$option": [
                                                                {
                                                                    "text": "编辑",
                                                                    "executor": "fnEdit"
                                                                },
                                                                "divider",
                                                                {
                                                                    "text": "删除",
                                                                    "executor": "fnDelete",
                                                                    "confirm": "确认删除选择的实体记录？"
                                                                }
                                                            ]
                                                        },
                                                        "name,字段名",
                                                        {
                                                            "metadata": "type,字段类型,MAPPING",
                                                            "$mapping": {
                                                                "java`lang`String": "字符串",
                                                                "java`lang`Integer": "整数",
                                                                "java`lang`Long": "长整数",
                                                                "java`lang`Boolean": "布尔值",
                                                                "java`math`BigDecimal": "浮点数",
                                                                "java`time`LocalTime": "时间格式",
                                                                "java`time`LocalDate": "日期格式",
                                                                "java`time`LocalDateTime":"日期/时间格式"
                                                            }
                                                        },
                                                        "columnName,数据库列名",
                                                        {
                                                            "metadata": "columnType,数据库类型,MAPPING",
                                                            "$mapping": {
                                                                "STRING1": "字符串",
                                                                "STRING2": "长文本",
                                                                "INT": "整数",
                                                                "LONG": "长整数",
                                                                "BOOLEAN": "布尔值",
                                                                "DECIMAL": "精度浮点数",
                                                                "DATE1": "日期格式",
                                                                "DATE2": "日期/时间格式",
                                                                "DATE3": "时间戳",
                                                                "DATE4": "时间格式",
                                                                "JSON": "JSON格式",
                                                                "XML": "XML格式",
                                                                "SCRIPT": "扩展脚本",
                                                                "BINARY": "二进制"
                                                            }
                                                        },
                                                        {
                                                            "metadata": "isPrimary,主键？,LOGICAL",
                                                            "$mapping": {
                                                                "true": "是",
                                                                "false": "否"
                                                            }
                                                        },
                                                        {
                                                            "metadata": "isNullable,可为空？,LOGICAL",
                                                            "$mapping": {
                                                                "true": "是",
                                                                "false": "否"
                                                            }
                                                        },
                                                        "length,长度",
                                                        "precision,精度",
                                                        "format,时间格式"
                                                    ]
                                                }
                                            }
                                        }
                                    ]
                                ]
                            },
                            "tabKey": {
                                "ui": [
                                    [
                                        {
                                            "metadata": "keys,,24,,aiDialogEditor",
                                            "optionJsx.config": {
                                                "form": "FormKey",
                                                "dialog": "键设置,保存,关闭,false,900,true,$opSaveKey",
                                                "op": {
                                                    "add": "$opShowKey"
                                                },
                                                "table": {
                                                    "columns": [
                                                        {
                                                            "title": "操作",
                                                            "dataIndex": "key",
                                                            "fixed": "left",
                                                            "$render": "EXECUTOR",
                                                            "$option": [
                                                                {
                                                                    "text": "编辑",
                                                                    "executor": "fnEdit"
                                                                },
                                                                "divider",
                                                                {
                                                                    "text": "删除",
                                                                    "executor": "fnDelete",
                                                                    "confirm": "确认删除选择的实体记录？"
                                                                }
                                                            ]
                                                        },
                                                        "name,键名",
                                                        {
                                                            "metadata": "type,键类型,MAPPING",
                                                            "$mapping": {
                                                                "PRIMARY": "主键",
                                                                "UNIQUE": "唯一键"
                                                            }
                                                        },
                                                        "columns,包含的列,ARRAY"
                                                    ]
                                                }
                                            }
                                        }
                                    ]
                                ]
                            }
                        },
                        "span": 24
                    }
                }
            ],
            [
                {
                    "metadata": "$button",
                    "hidden": true,
                    "optionJsx.extension": [
                        "$opSave,保存,SUBMIT,primary",
                        "$opDelete,删除,KEY",
                        "$opReset,重置,RESET"
                    ],
                    "span": 24
                }
            ]
        ],
        "hidden": [
            "key",
            "namespace"
        ],
        "op": {
            "$opSave": "act.entity.save",
            "$opDelete": "act.entity.delete"
        }
    },
    "_modal": {
        "success": {
            "saved": "您好，您的实体信息已经更新成功！",
            "removed": "当前实体记录删除成功！"
        }
    }
}