{
    "_form": {
        "window": 1,
        "columns": 2,
        "ui": [
            [
                {
                    "metadata": "name,字段名称,,,,placeholder=必填",
                    "optionConfig.rules": [
                        "required,请输入字段名称！",
                        {
                            "validator": "duplicatedDatum",
                            "message": "对不起，您输入的字段名称出现重复！",
                            "config": {
                                "datum": "form.fields"
                            }
                        }
                    ]
                },
                {
                    "metadata": "type,数据类型,,,aiSelect,placeholder=必选",
                    "optionJsx.config.items": [
                        {
                            "key": "java.lang.String",
                            "label": "字符串"
                        },
                        {
                            "key": "java.lang.Integer",
                            "label": "整数"
                        },
                        {
                            "key": "java.lang.Long",
                            "label": "长整型"
                        },
                        {
                            "key": "java.lang.Boolean",
                            "label": "布尔值"
                        },
                        {
                            "key": "java.math.BigDecimal",
                            "label": "浮点数"
                        },
                        {
                            "key": "java.time.LocalTime",
                            "label": "时间格式"
                        },
                        {
                            "key": "java.time.LocalDate",
                            "label": "日期格式"
                        },
                        {
                            "key": "java.time.LocalDateTime",
                            "label": "日期/时间格式"
                        }
                    ],
                    "optionConfig.rules": [
                        "required,请选择数据类型！"
                    ]
                }
            ],
            [
                {
                    "metadata": "columnName,列名,,,,placeholder=必填",
                    "optionConfig.rules": [
                        "required,请输入数据库列名！",
                        {
                            "validator": "duplicatedDatum",
                            "message": "对不起，您输入的数据库列名出现重复！",
                            "config": {
                                "datum": "form.fields"
                            }
                        }
                    ]
                },
                {
                    "metadata": "columnType,列类型,,,aiSelect,placeholder=必选",
                    "optionJsx.config.items": [
                        "STRING1,字符串",
                        "STRING2,长文本",
                        "INT,整数",
                        "LONG,长整数",
                        "BOOLEAN,布尔值",
                        "DECIMAL,精度浮点数",
                        "DATE1,日期格式",
                        "DATE2,日期/时间格式",
                        "DATE3,时间戳",
                        "DATE4,时间格式",
                        "JSON,JSON格式",
                        "XML,XML格式",
                        "SCRIPT,扩展脚本",
                        "BINARY,二进制"
                    ],
                    "optionConfig.rules": [
                        "required,请选择数据库列类型！"
                    ]
                }
            ],
            [
                {
                    "metadata": "length,长度,,,aiInputNumber",
                    "optionJsx.depend.enabled": {
                        "columnType": [
                            "STRING1",
                            "DECIMAL"
                        ]
                    }
                },
                {
                    "metadata": "precision,精度,,,aiInputNumber",
                    "optionJsx.depend.enabled": {
                        "columnType": "DECIMAL"
                    }
                }
            ],
            [
                {
                    "metadata": "primary,主键？,,,aiCheckbox",
                    "optionConfig": {
                        "valuePropName": "checked"
                    },
                    "optionJsx.trigger.impact": {
                        "true": {
                            "nullable": false
                        }
                    }
                },
                {
                    "metadata": "nullable,可为空？,,,aiCheckbox",
                    "optionConfig": {
                        "valuePropName": "checked"
                    },
                    "optionJsx.depend.enabled": {
                        "primary": false
                    }
                }
            ],
            [
                {
                    "metadata": "format,时间格式,,,aiSelect",
                    "optionJsx.depend.enabled": {
                        "columnType": [
                            "DATE1",
                            "DATE2",
                            "DATE3",
                            "DATE4"
                        ]
                    },
                    "optionJsx.config.items": [
                        {
                            "key": "yyyy-MM-dd HH:mm:ss",
                            "label": "标准格式（yyyy-MM-dd HH:mm:ss）"
                        },
                        {
                            "key": "yyyy-MM-dd",
                            "label": "日期格式（yyyy-MM-dd）"
                        }
                    ]
                }
            ],
            [
                "comments,备注,24,,aiTextArea,rows=3"
            ],
            [
                {
                    "metadata": "$button",
                    "hidden": true,
                    "optionJsx.extension": [
                        "$opSaveField,保存,SAVE_ROW,primary",
                        "$opReset,重置,RESET"
                    ],
                    "span": 24
                }
            ]
        ],
        "hidden": [
            "key",
            "entityId"
        ],
        "op": {
            "$opSaveField": "$CLIENT$"
        }
    },
    "_modal": {
        "error": {
            "duplicated": "对不起，您的数据出现了重复，无法添加！"
        }
    }
}